<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="applicationController.ViewTypeVehicleForAdminController">
   <children>
      <Pane prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: black;">
         <children>
            <TableView fx:id="typeVehicleTable" prefHeight="500.0" prefWidth="300.0">
              <columns>
                <TableColumn fx:id="columnTypeVehicle" prefWidth="150.0" style="-fx-alignment: center;" text="Type Vehicle" />
                <TableColumn fx:id="columnPriceForHour" prefWidth="150.0" style="-fx-alignment: center;" text="Price For Hour" />
              </columns>
            </TableView>
            <JFXButton fx:id="addTypeVehButtonID" layoutX="309.0" layoutY="70.0" onAction="#addNewTypeVehButton" prefHeight="61.0" prefWidth="177.0" style="-fx-border-color: white;" text="Add Type Vehicle" textFill="WHITE">
               <font>
                  <Font name="Verdana Italic" size="17.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="deleteTypeButtonID" layoutX="309.0" layoutY="271.0" onAction="#deleteTypeButton" prefHeight="61.0" prefWidth="177.0" style="-fx-border-color: white;" text="Delete Type" textFill="WHITE">
               <font>
                  <Font name="Verdana Italic" size="18.0" />
               </font>
            </JFXButton>
            <JFXButton layoutX="309.0" layoutY="370.0" onAction="#closeButton" prefHeight="61.0" prefWidth="177.0" style="-fx-border-color: white;" text="Close" textFill="WHITE">
               <font>
                  <Font name="Verdana Italic" size="18.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="changeCostButtonID" layoutX="309.0" layoutY="171.0" onAction="#changeCostButton" prefHeight="61.0" prefWidth="177.0" style="-fx-border-color: white;" text="Change Cost" textFill="WHITE">
               <font>
                  <Font name="Verdana Italic" size="18.0" />
               </font>
            </JFXButton>
         </children>
      </Pane>
   </children>
</StackPane>
